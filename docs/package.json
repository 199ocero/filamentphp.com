{
  "name": "@filament/docs",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "checkout": "bin/checkout-latest-filament",
    "sync": "npm run checkout && node sync.mjs",
    "torchlight": "npx torchlight --config torchlight.config.cjs",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "generate": "npm run sync && npm run build && npm run torchlight && rm -rf ../public/docs && mv dist ../public/docs",
    "generate-dev": "node sync.mjs && npm run build && rm -rf ../public/docs && mv dist ../public/docs",
    "generate-dev-highlight": "node sync.mjs && npm run build && npm run torchlight && rm -rf ../public/docs && mv dist ../public/docs",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^0.13.0",
    "@astrojs/react": "^1.2.2",
    "@astrojs/tailwind": "^2.1.3",
    "@docsearch/js": "^3.3.1",
    "astro": "^1.7.2",
    "astro-icon": "^0.8.0"
  },
  "devDependencies": {
    "@alpinejs/focus": "^3.10.5",
    "@awcodes/alpine-floating-ui": "^3.4.0",
    "@grafite/helpers": "^1.0.2",
    "@tailwindcss/typography": "^0.5.8",
    "@torchlight-api/torchlight-cli": "^0.1.7",
    "alpinejs": "^3.10.5",
    "autoprefixer": "^10.4.13",
    "fs-extra": "^11.1.0",
    "heroicons": "^2.0.13",
    "postcss-focus-visible": "^7.1.0",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.2.4"
  }
}
